//debug yes;
movingMesh no;
IO {
  IOType no;
  writeFields (
	       O
	       C
	       N
	       N2
	       O2
	       NO
	       C3
	       C2
	       CN
	       CO2
	       CO
	       );
  probingFunctions
  (
  );
}
MaterialProperties
{
  MaterialPropertiesType Porous; 
  MaterialPropertiesDirectory "$FOAM_CASE/DATA";
}

Mass
{
  MassType DarcyLaw; 
}

Energy
{
  EnergyType PureConduction;
}

GasProperties
{
  GasPropertiesType FiniteRate; 
}

MaterialChemistry
{
  MaterialChemistryType SpeciesConservation;
  mixture air11_carbon_park; 
}
VolumeAblation
{
  VolumeAblationType FibrousMaterialTypeA; // USELESS BUT NECESSARY FOR INIT
