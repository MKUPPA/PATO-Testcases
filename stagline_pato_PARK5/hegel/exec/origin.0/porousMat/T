FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 298;

boundaryField
{
  outer
    {
      type   fixedGradient; // since q from hegel
      gradient  uniform 0;
    }
  back
    {
      type   zeroGradient;
    }
  inner
    {
      type   fixedValue;
      value  $internalField;
    }
  axis
    {
      type   empty;
    }
  wedge-left
    {
      type   wedge;
    }
  wedge-right
    {
      type   wedge;
    }
}

// ************************************************************************* //
