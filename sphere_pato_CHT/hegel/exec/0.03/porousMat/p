FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.03/porousMat";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 1 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    axis
    {
        type            empty;
    }
    back
    {
        type            zeroGradient;
    }
    wedge-right
    {
        type            wedge;
    }
    wedge-left
    {
        type            wedge;
    }
    outer
    {
        type            fixedValue;
        value           uniform 101325;
    }
    inner
    {
        type            fixedValue;
        value           uniform 101325;
    }
}


