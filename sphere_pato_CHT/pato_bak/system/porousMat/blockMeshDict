 (-0.300000 0.000000 -0.000001)   // 4
 (0.000000 0.298858 -0.026148)   // 5
 (0.000000 0.149429 -0.013074)   // 6
 (-0.150000 0.000000 -0.000001)   // 7
 );
blocks
( 
 // WEDGE FRONT
 hex (0 1 2 3 4 5 6 7) (50 100 1) simpleGrading  (1 10 1)
  );

edges
(
 // ARC CONNECTION
 arc 0 1  (-0.212132 0.211325 0.018490)
 arc 4 5  (-0.212132 0.211325 -0.018490)
 arc 2 3  (-0.106066 0.105662 0.009245)
 arc 6 7  (-0.106066 0.105662 -0.009245)
 );

boundary
( 
