 {
   type wedge;
   faces
     (
      (0 1 2 3)
      );
 }
 wedge-left
 {
   type wedge;
   faces
     (
      (4 5 6 7)
      );
 }
 outer
 {
   type wall ;
   faces
     (
      (0 1 5 4)
      );
 }
 inner
 {
   type wall ;
   faces
     (
      (2 3 7 6)
      );
 }
  );

mergePatchPairs
(
);

