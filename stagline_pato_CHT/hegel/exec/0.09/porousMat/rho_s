FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.09/porousMat";
    object      rho_s;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 280;

boundaryField
{
    axis
    {
        type            empty;
    }
    back
    {
        type            calculated;
        value           uniform 280;
    }
    wedge-right
    {
        type            wedge;
    }
    wedge-left
    {
        type            wedge;
    }
    outer
    {
        type            calculated;
        value           uniform 280;
    }
    inner
    {
        type            calculated;
        value           uniform 280;
    }
}


